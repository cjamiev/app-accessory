{
  "name": "app-accessory",
  "version": "0.3.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"ng serve --aot --proxy-config client/config/proxy.config.json\" \"node server/src/index.js\"",
    "client": "ng serve --aot --proxy-config client/config/proxy.config.json",
    "server": "babel-node server/src/index.js",
    "server-pm2": "pm2 start --name app-accessory server/src/index.js --watch -o server/tmp/pm2.log -e server/tmp/pm2.err",
    "build": "ng build",
    "test": "ng test",
    "jest": "jest ./server/__tests__ --watch",
    "coverage": "jest ./server/__tests__ --coverage",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.2.0",
    "@angular/common": "7.2.0",
    "@angular/compiler": "7.2.0",
    "@angular/core": "7.2.0",
    "@angular/forms": "7.2.0",
    "@angular/platform-browser": "7.2.0",
    "@angular/platform-browser-dynamic": "7.2.0",
    "@angular/router": "7.2.0",
    "core-js": "2.5.4",
    "cors": "2.8.5",
    "express": "4.16.4",
    "jest": "^24.7.1",
    "rxjs": "6.3.3",
    "tslib": "1.9.0",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/preset-env": "^7.4.4",
    "@angular-devkit/build-angular": "0.13.0",
    "@angular/cli": "7.3.7",
    "@angular/compiler-cli": "7.2.0",
    "@angular/language-service": "7.2.0",
    "@types/node": "8.9.4",
    "@types/jasmine": "2.8.8",
    "@types/jasminewd2": "2.0.3",
    "babel-plugin-module-resolver": "^3.2.0",
    "codelyzer": "4.5.0",
    "concurrently": "4.1.0",
    "jasmine-core": "2.99.1",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.1",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "0.2.2",
    "pm2": "3.4.1",
    "prettier": "1.16.4",
    "protractor": "5.4.0",
    "ts-node": "7.0.0",
    "tslint": "5.11.0",
    "typescript": "3.2.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "./"
          ]
        }
      ],
      "@babel/plugin-proposal-class-properties"
    ]
  }
}
